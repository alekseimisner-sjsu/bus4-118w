
body{
    margin: 0;
    padding: 0;
    font-family: Poppins;
}
h1{
    font-size: 50px;
    font-family: cursive;
}
h2{
  font-size: 25px;
  font-family: cursive;
}
h3{
    font-size: 30px;
    font-family: cursive;
}
h4{
  font-size: 15px;
  font-family: cursive;
}
p{
    font-size: 20px;
    font-family: Arial, Helvetica, sans-serif; 
}
.footer {
  padding: 10px;
  text-align: center;
  background: #ddd;
  /* position: absolute; */
  bottom:0;
  width:100%;
  height:85px;
}
.header {
  padding:65px;
  text-align: center;
  background: #61b1e5;
  color: black;
  height: 250px;
}
/* navbar */
.navbar{
    float: right;
    position: fixed;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    flex-wrap: wrap;
    background-color: whitesmoke;
    width: 100%;
    height: 70px;
    z-index: 1;
}
.nav{
    display: flex;
    justify-content: right;
    list-style: none;
    margin-right: 15%;
}
.nav li{
    display: inline-block;
}
.nav li a{
    color:#000 lightgray;
    text-decoration: none;
    padding:5px 10px;
    font-size: 20px;
}
.nav li a:hover{
    background:#399ddfab;
    color: rgb(17, 6, 6);
}
.logo{
    flex: 1 1 auto;
    margin-left: 10%;
    text-transform: uppercase;
    letter-spacing: 1px;
    font-weight: bold;
    font-size: 35px;
    font-family: cursive;
}
a{
    margin: 15px;
    color: #000;
    text-decoration: none;
    text-transform: uppercase;
}
a:hover{
    color: #000; 
}
